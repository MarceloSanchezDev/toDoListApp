{
  "expo": {
    "name": "toDoListApp",
    "slug": "toDoListApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/grafico-de-barras.png",
    "scheme": "todolistapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#fffff1",
        "backgroundImage": "./assets/images/grafico-de-barras.png"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/grafico-de-barras.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
  {
    "image": "./assets/images/grafico-de-barras.png",
    "imageWidth": 200,
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
